{
  "manifest_version": 2,
  "name": "__MSG_EXTENSION_NAME__",
  "short_name": "__MSG_EXTENSION_NAME__",
  "version": "2.1.0",
  "description": "__MSG_EXTENSION_DESCRIPTION__",
  "author": "kain",
  "developer": {
    "name": "kain",
    "url": "https://www.teamclerks.net/historyblock"
  },
  "default_locale": "en",
  "homepage_url": "https://github.com/kainsavage/HistoryBlock",
  "applications": {
    "gecko": {
      "id": "historyblock@teamclerks.net",
      "strict_min_version": "55.0"
    }
  },
  "permissions": [
    "sessions",
    "tabs",
    "history",
    "storage",
    "contextMenus",
    "cookies",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "libraries/i18n.js",
      "libraries/RuntimeAction.js",
      "libraries/hash/NoHash.js",
      "libraries/hash/SHA1.js",
      "libraries/match/psl.min.js",
      "libraries/match/Matcher.js",
      "libraries/match/DomainMatcher.js",
      "libraries/match/SubdomainMatcher.js",
      "libraries/match/URLMatcher.js",
      "libraries/ContextMenu.js",
      "historyblock.js"
    ]
  },
  "icons": {
    "16": "icons/HistoryBlock16x16.png",
    "32": "icons/HistoryBlock32x32.png",
    "64": "icons/HistoryBlock64x64.png",
    "128": "icons/HistoryBlock128x128.png",
    "512": "icons/HistoryBlock512x512.png"
  },
  "options_ui": {
    "page": "options/index.html",
    "browser_style": true
  }
}